      "title": "(.*)\\s\\(.*\\)",
      "directory": "",
      "template": "[{authors}] {title}"
    },
    {
      "//": "デフォルト",
      "authors": ".*",
      "title": ".*",
      "directory": "",
      "template": "[{authors}] {title}"
    }
  ]
